use uo;
use util;
use os;

program macehit(param)
	var attacker := param[1];
	var defender := param[2];
	var rawdamage := param[6];

	if ((attacker.dead) || (defender.dead))
		return;
	endif
	if (!defender.acctname)
		rawdamage := (rawdamage / 2);
	endif
	SetObjProperty(defender, "TempDamage", rawdamage);

	return;
endprogram

   
