use uo;

include "include/tileEffects";

program textcmd_e(who)

  PlayObjectCenteredEffect(who, 0x3709, 0x0a, 0x1e );
  PlayObjectCenteredEffect(who, FX_SPARK_EFFECT, 7, 0x10 );
  PlaySoundEffect(who,0xf8);
  if( who.concealed > 0 )
    who.concealed := 0;
  else
    who.concealed := who.cmdlevel;
  endif

endprogram
