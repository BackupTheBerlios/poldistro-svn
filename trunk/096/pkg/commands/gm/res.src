use os;
use uo;
use boat;

include "include/messages";
include "include/localization";

program command_res(who, params)
	if ( who.dead )
		Resurrect(who);
	else
		var targ := Target(who);
		if ( targ.dead )
			Resurrect(targ);
		endif
	endif
endprogram
	