/* $Id$
 *
 * Purpose
 * This script allows the scripter to run code when a character is deleted.
 *
 */
use uo;

include "include/OLD/startEqp";


program core_onDelete(who)
	var created := GetObjProperty(who, "OnCreate");
	if (!created)
		created := 0;
	endif
	if ((ReadGameClock() - created) >= 604800)
		print("deleting char");
		return 1;
	else
		print("too young");
		return 0;
	endif
endprogram
