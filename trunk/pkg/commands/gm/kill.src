use os;
use uo;
use boat;

include "include/attributes";


program command_kill(who)
	var tgt := TargetCoordinates( who );
	if (tgt.mobile)
		var mob := tgt.mobile;
		if (!mob.dead)
			RevokePrivilege(mob, "invul");
			SetObjProperty(mob, "guardkill", 1);
			ApplyRawDamage(mob, GetHp(mob));
		else
			SendSysMessage(who, "That tickles!");
		endif
	endif

	return;
endprogram
