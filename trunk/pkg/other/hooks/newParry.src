exported function NewParryAdvancement( attacker, weapon, defender, shield )
	attacker := attacker;	// stops ecompile "not used" warning.
	weapon := weapon;		// as above
	shield := shield;		// as above

	AwardPoints( defender, SKILLID_PARRY, 20 );

endfunction